/* slashes */
div.slash {
    &:before,
    &:after {
        position: absolute;
        top: 0;
        left:0;
        width: 0;
        height: 0;
        border-style: solid;
        content: '';
    }

    &.top {
        top: 0;
        &:after {
            border-width: 25px 100vw 0 0;
            border-color: currentColor transparent transparent transparent;
        }
    }

    &.bottom {
        bottom: 0;
        &:after {
            border-width: 0 0 25px 100vw;
            border-color: transparent transparent currentColor transparent;
        }
    }

}



section {

    width: 100vw;

    &.slash {

        &:before {
            position: absolute;
            top: -2rem;
            left: 0;
            width: calc(100vw + 2rem);
            overflow: hidden;
            display: flex;
            transform: rotate(-2deg) skew(-2deg);
            background-color: inherit;
            margin: 0 -1em;
            height: 3.25rem;
            content: '';
        }

        &:after {
            position: absolute;
            bottom: -2rem;
            left: 0;
            width: calc(100vw + 2rem);
            overflow: hidden;
            display: flex;
            transform: rotate(-2deg) skew(-2deg);
            background-color: inherit;
            margin: 0 -1em;
            height: 3.25rem;
            content: '';
        }

    }

    &.primary {
        background-color: var(--color-white);
        color: var(--color-gray-600);

        h1,h2,h3,h4,h5,h6 {
            color: var(--color-kmm-dark-blue);
        }

    }

    &.secondary {
        background-color: var(--color-kmm-dark-blue);
        color: var(--color-white);

        h1,h2,h3,h4,h5,h6 {
            color: var(--color-kmm-dark-blue-80);
        }

    }

    &.full-width {

        h1, h2, h3, h4, h5, h6, p {
            width: 100vw;
        }

    }

    &.boxed {

        h1, h2, h3, h4, h5, h6, p {
            width: 100%;
            @screen sm { max-width: 640px; }
            @screen md { max-width: 768px; }
            @screen lg { max-width: 1024px; }
            @screen xl { max-width: 1280px; }
            @screen 2xl { max-width: 1536px; }
        }

    }


}







