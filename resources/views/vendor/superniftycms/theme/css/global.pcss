 ul.media.slash {

    position: relative;
    width: calc(100vw + 2em);
    user-select: none;
    z-index: 25;
    margin: 0 -1rem;
    padding: 0 2rem;
    display: flex;
    list-style: none;

    overflow: hidden;
    transform: rotate(-2deg) skew(-2deg);

    li {
        display: flex;
        justify-content: center;
        align-items: center;
        width: 100%;
        margin-right: 1rem;
        &:last-child {
            margin-right: 0;
        }
        object-fit: cover;
        color: inherit;
        background-color: currentColor;
        border-radius: .5em;
        overflow: hidden;
        box-shadow: 1px 2px 3px rgba(0,0,0,.25);

        img {
            width: 100%;
            height: 250px;
            object-fit: cover;
            opacity: .5;
            filter: saturate(.5);
            cursor: pointer;
            transition: all 250ms ease-in-out;

            &:hover {
                filter: saturate(1);
                opacity: 1;
                cursor: zoom-in;
                transform: scale(1.25);
            }
        }
    }
}


.media-text {

    margin-left: auto;
    margin-right: auto;
    max-width: 56rem;
    display: flex;
    padding-left: 3rem;
    padding-right: 3rem;
    pointer-events: none;
    user-select: none;

    img {
        width: 200px;
        min-width: 200px;
        height: 200px;
        object-fit: cover;
        opacity: .5;
        filter: saturate(.5);
        border-radius: 100%;
        overflow: hidden;
        margin-right: 5vw;
    }

    div {
        display: flex;
        flex-direction: column;
