i.process {

    position: absolute;
    top: -1vw;
    right: -1vw;
    display: flex;
    justify-content: center;
    align-items: center;
    width: 2vw;
    height: 2vw;
    font-size: 2vw;
    color: var(--color-blue-300);
    font-style: normal;
    margin: 0;
    padding: 0;

    &:before, &:after {
        position: absolute;
        inset: 0;
        display: flex;
        justify-content: center;
        align-items: center;
        width: 2.5vw;
        height: 2.5vw;
        font-family: 'Material Symbols Outlined';
        font-variation-settings: 'FILL' 1, 'wght' 700, 'GRAD' 200, 'opsz' 22;
        font-size: inherit;
        opacity: .75;
        color: inherit;
    }

    &:before {
        content: '\e917';
        font-size: 3em;
        animation: cws 15s linear infinite;
    }

    &:after {
        content: '\f560';
        font-size: 1.5em;
        animation: ccws 10s linear infinite;
    }

    @keyframes ccws {
        0%   { transform: rotate(0deg); }
        50%  { transform: rotate(-180deg); }
        100% { transform: rotate(-360deg); }
    }
    @keyframes cws {

        0%   { transform: rotate(0deg); }
        50%  { transform: rotate(180deg); }
        100% { transform: rotate(360deg); }

    }


}


.body {

    h1, h2, h3, h4, h5, h6 {
        text-transform: none;
    }

}

.body p,
.box-links {

    a,
    a:link,
    a:visited {
        position: relative;
        font-weight: 700;
        color: var(--color-kmm-light-blue)!important;
        &:before {
            content: '';
            position: absolute;
            bottom: 15%;
            left: -2px;
            width: calc(100% + 4px);
            height: 10px;
            border-radius: 2px;
            background-color: var(--color-kmm-light-blue);
            z-index: -1;
            transition: all 250ms ease-in-out;
            opacity: .25;
        }

    }

    a:hover,
    a:focus,
    a:active {

        &:before {
            left: -4px;
            width: calc(100% + 8px);
            bottom: 0;
            height: 100%;
        }

    }

    &.dark {
        a,
        a:link,
        a:visited {
            color: var(--color-kmm-green)!important;
            &:before {
                background-color: var(--color-kmm-green);
            }

        }

        a:hover,
        a:focus,
        a:active {
            color: var(--color-kmm-light-blue)!important;
            &:before {
                background-color: var(--color-white)!important;
                opacity: 1;

            }
        }
    }

}



.box {
    position: relative;
    display: flex;
    flex-direction: column;
    border-radius: 0 0 .5em .5em;
    box-shadow: 1px 2px 3px rgba(0,0,0,.25);

    ul, ol {
        padding: 0;
        margin: 4em 0;

        li {
            position: relative;
            display: flex;
            align-items: center;
            line-height: 1;
            font-size: .75em;
            padding: 1em 2em 1em 1em;
            border-top: 1px solid rgba(255,255,255,.1);
            border-bottom: 1px solid rgba(0,0,0,.1);
            margin: 0;
            transition: all 250ms ease-in-out;
            cursor: pointer;

            &:first-child { border-top: none; }
            &:last-child { border-bottom: none; }

            &:before {
                font-family: 'Material Symbols Outlined';
                font-variation-settings: 'FILL' 1, 'wght' 700, 'GRAD' 200, 'opsz' 22;
                content: '\f1df';
                margin-right: 1em;
                transition: all 250ms ease-in-out;
            }

            &:hover {
                &:before {
                    transform: scale(1.5);
                }
            }
        }





    }

    ul {
        list-style:none;
    }

    a,
    a:link,
    a:visited {

        display: flex;
        justify-content: center;
        background-color: var(--color-green-500)!important;
        color: var(--color-white)!important;
        padding: .5em 1em;
        border-radius: .25em;
        font-weight: 700;
        text-transform: uppercase;
        text-align: center;
        margin: 1em 1em 1em auto;


    }


}


.slice {
    display: flex;
    align-items: center;
    background-color: currentColor;
    height: 100px;
    width: calc(100vw + 2em);
    position: relative;
    user-select: none;
    transform: rotate(-2deg) skew(-2deg);
    margin: -2em -1em;
    z-index: -1;

    a,
    a:link,
    a:visited
    {
        position: absolute;
        right: 10vw;
        top: -1.5rem;
        display: flex;
        align-items: center;
        transform: rotate(2deg) skew(2deg);
        color: var(--color-white);
        background-color: var(--color-green-500);
        padding: .875em 2em .875em 3em;
        border-radius: .25em;
        box-shadow: 1px 2px 3px rgba(0,0,0,.25);
        margin: 0 5vw 0 auto;
        z-index: 10;
        font-size: 1rem;
        font-weight: 700;
        text-transform: uppercase;
        transition: all 250ms ease-in-out;
        line-height: 1;

        &:before {
            display: none;
        }

        &:after {
            display:inline-block;
            font-family: 'Material Symbols Outlined';
            font-variation-settings: 'FILL' 1, 'wght' 700, 'GRAD' 200, 'opsz' 22;
            content: '\f1df';
            margin: 2px 0 0 1em;
            transition: all 250ms ease-in-out;
        }

    }

    a:hover,
    a:focus,
    a:active
    {
        background-color: var(--color-kmm-light-blue)!important;
        transform: rotate(2deg) skew(2deg) scale(1.1)!important;

    }

}


a.cta-button,
