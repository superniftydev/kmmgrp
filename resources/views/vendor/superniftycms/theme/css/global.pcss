html {
    font-family: var(--font-family-sans-serif);
    font-size: var(--font-size);

    @screen sm { font-size: 110%; }
    @screen md { font-size: 120%; }
    @screen lg { font-size: 130%; }
    @screen xl { font-size: 140%; }

    color: var(--color-body);
    background-color: var(--color-body-bg);
    background-size: cover;
    background-blend-mode: darken;
    scroll-behavior: smooth;
    overflow-x: hidden;
}

body {
    background-color: var(--color-body-bg);
    color: var(--color-body);
    font-family: var(--font-family-sans-serif);
    line-height: var(--font-line-height);
    transition: background-color 250ms ease-in-out, color 250ms ease-in-out;
    overflow-x: hidden;
}

main {
    position: relative;
    display: flex;
    flex-direction: column;
    align-items: center;
    text-align: center;
    width: 100vw;
    min-height: 100vh;

    section {
        position: relative;
        z-index: 1;
        width: 100vw;
    }

    &.starter-template {

        position: fixed;
        inset: 0;
        display: flex;
        justify-content: center;
        align-items: center;
    }

}

button {
    display: flex;
    font-size: 1.15em;
    font-weight: 700;
    background-color: rgba(255,255,255,1);
    color: rgba(0,0,0,1);
    padding: .75em 3em;
    border-radius: .25em;
    margin: .5em 0 0 auto;
    transition: inherit;
    text-transform: uppercase;
    &:hover {
        transform: scale(1.05);
    }
    &:focus,
    &:active {
        outline: none;
        box-shadow: none;
    }

}


.container {
    @apply flex flex-col justify-center max-w-4xl mx-auto px-12;
    @screen xl {
        @apply px-0;
    }

    section {
        width: auto;
    }
}

header {
    display: flex;
    position: relative;
    flex-direction: column;
    width: 100vw;
    padding: 10em 0;
    background-color: var(--color-kmm-dark-blue);
    h1 {
        color: var(--color-white);
        font-size: 5em;
        line-height: .9;
        margin: 0;
    }

}

/* see: resources/views/components/sprites.blade.php */
#fa-x-twitter,
#fa-instagram,
#fa-youtube,
#fa-linkedin,
#snCircle
{ fill: var(--color-white); }


#kmm-logo-white { fill: var(--color-white); }
#kmm-logo-dark-blue { fill: var(--color-kmm-dark-blue); }
#kmm-logo-medium-blue { fill: var(--color-kmm-medium-blue); }
#kmm-logo-light-blue { fill: var(--color-kmm-light-blue); }



/*
vite does not watch for html changes on supernifty because that causes unwanted page reloads.
so it makes more sense to register commonly used tailwind classes here so that they
will be available to the front end.
*/


