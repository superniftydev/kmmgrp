:root {
    --welcome-header-background-height: 700px;
    --welcome-header-headline-height: 450px;
    --welcome-header-model-height: 700px;
    --welcome-header-cta-height: 460px;
}


body[data-topic_url='welcome'] {

    h2 {
        text-align: center;
    }

    p {
        font-size: 1.5em;
    }

    main {
        position: relative;
    }

    #mission-statement {
        position: relative;
        padding: 4rem 0 0 0;
        color: var(--color-white);
        background-color: var(--color-kmm-dark-blue);

        p {
            font-size: 1em;
            color: var(--color-white);
        }
    }

    #capabilities {

        padding: 0;
        background-color: var(--color-kmm-dark-blue);

        ul {

            list-style: none;
            margin: 0;
            padding: 1em;
            @apply grid grid-cols-1 md:grid-cols-2 xl:grid-cols-3 gap-0 place-content-center;

            li {

                position: relative;
                transition: all 250ms ease-in-out;

                a,
                a:link,
                a:visited {
                    position: relative;
                    z-index: 2;
                    display: flex;
                    flex-direction: column;
                    align-items: center;
                    justify-content: center;
                    margin: 0;
                    padding: 1em;
                    width: 100%;

                    &:before {
                        position: absolute;
                        top: 50%;
                        bottom: 50%;
                        left: 0;
                        right: 0;
                        display: flex;
                        justify-content: center;
                        align-items: center;
                        transition: all 250ms ease-in-out;
                        background-color: var(--color-kmm-dark-blue-30);
                        height: 1px;
                        width: 100%;
                        opacity: 0;
                        content: '';
                    }

                    model-viewer {
                        position: relative;
                        z-index: 1;
                        transition: all 250ms ease-in-out;
                        margin: 0;
                        width: 25vw;
                        height: 25vw;
                        opacity: .6;
                        pointer-events: none;
                    }

                    h3 {
                        position: absolute;
                        inset: 0;
                        z-index: 4;
                        display: flex;
                        justify-content: center;
                        align-items: center;

                        strong {
                            font-style: normal;
                            color: var(--color-white);
                            font-size: .625em;
                            text-align: center;
                            margin: 5vw auto -5vw auto;
                            font-weight: 700;
                            line-height: .9;
                            white-space: nowrap;
                            transition: all 250ms ease-in-out;
                            background-color: var(--color-green-500);
                            text-transform: uppercase;
                            padding: .5em .75em .5em 1.25em;
                            border-radius: .25em;
                            box-shadow: 1px 2px 3px rgba(0,0,0,.3);
                            display: flex;
                            align-items: center;

                            i {
                                font-style: normal;
                                text-align: right;
                            }

                            &:after {
                                display:inline-block;
                                font-family: 'Material Symbols Outlined';
                                font-variation-settings: 'FILL' 1, 'wght' 700, 'GRAD' 200, 'opsz' 22;
                                content: '\f1df';
                                margin: 0 0 0 .5em;
                                transition: all 250ms ease-in-out;
                            }

                        }

                    }

                    i.c {

                        position: absolute;
                        inset: 0;
                        z-index: 2;
                        display: flex;
                        justify-content: center;
                        align-items: center;
                        pointer-events: none;
                        mix-blend-mode: darken;

                        &:before {
                            background-color: var(--color-kmm-dark-blue);
                            content: '';
                            width: 1px;
                            height: 1px;
                            border-radius: 100%;
                            transition: all 250ms ease-in-out;
                            opacity: 0;
                        }

                    }

                }

                &.ctaW {
                    display: flex;
                    flex-direction: column;
                    opacity: 1;
                    justify-content: center;

                    a {

                        background-color: transparent;
                        &:before {
                            display: none;
                        }
                    }



                }

                &:hover {

                    a {

                        &:before {
                            height: 100%;
                            opacity: 1;
                            top: 0;
                            bottom: 0;
                            left: 0;
                            right: 0;
                            border-radius: .5em;
                        }

                        h3 {
                            strong {
                                transform: scale(1.25);
                                background-color: var(--color-green-600);
                                margin: 3vw auto -3vw auto;
                            }
                        }


                        model-viewer {
                            opacity: 1;
                            transform: rotate(-5deg);

                        }

                        i.c {

                            &:before {
                                width: 25vw;
                                height: 25vw;
                            }

                        }

                    }

                }

            }

        }


        model-viewer::part(default-progress-bar) {
            display: none;
        }

    }

    .cta-slash {
        margin-left: auto!important;
        margin-right: auto!important;
    }





    .client-logos {

        position: relative;
        display: flex;
        background-color: var(--color-kmm-dark-blue);
        width: 100vw;
        padding: 5em;

        .logo-grid {
            @apply grid md:grid-cols-2 lg:grid-cols-4 gap-8 my-12 w-full mx-auto;
        }

        img {
            width: 150px;
            height: 100px;
            margin: 0 auto;
        }

    }


    a.divider-cta,
    a.divider-cta:link,
    a.divider-cta:visited {

        position: absolute;
        bottom: -2rem;
        left: 30vw;

        display: flex;
        background-color: var(--color-white);
        color: var(--color-kmm-dark-blue);
        padding: .5rem 2rem;
        border-radius: .25rem;
        text-transform: uppercase;
        font-weight: 700;
        transition: all 250ms ease-in-out;

    }


    a.divider-cta:active,
    a.divider-cta:hover,
    a.divider-cta:focus {

        background-color: var(--color-green-400);
        color: var(--color-white);
        transform: scale(1.1);

    }


    #who-we-are {
        position: relative;
        padding: 2rem 0 4rem 0;
        color: var(--color-white);
        background-color: var(--color-kmm-dark-blue);
        p {
            font-size: .75rem;
            color: var(--color-white);
        }
    }


    #who-we-serve {
        padding: 8rem 0;
        background-color: var(--color-gray-100);
    }






}


