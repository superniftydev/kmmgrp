#homepage-slideshow {
    position: relative;
    width: 100vw;
    height: 50vh;
    margin-bottom: 10em;
}

.video-wrap {
    background-color: var(--color-gray-900);
    margin-bottom: 10em;
}

.vw {
    position: relative;
    display: flex;
    justify-content: center;
    align-items: center;
    width: 100vw;
    height: 75vh!important;
    flex-grow: 1;
    overflow: hidden;
    background-color: var(--color-kmm-dark-blue);

    .screen {
        position: absolute;
        inset: 0;
        z-index: 5;
    }

    video {
        position: relative;
        z-index: 3;
        width: 100%;
        height: 100%;
        object-fit: cover;
        opacity: .3;
    }

    #timer {
        position: absolute;
        bottom: 2.75em;
        left: 5.5em;
        z-index: 99999999999;
        border-radius: .25em;
        color: #fff;
        font-size: 1em;
        font-family: "IBM Plex Mono", monospace;
        background-color: rgba(255,255,255,.1);
        padding: .25rem .5rem;
        line-height: 1;
        border: 1px solid rgba(255,255,255,.1);
        user-select: none;
        pointer-events: none;
        display: none;
    }

    #playpause {
        display: none;
        position: absolute;
        bottom: 1rem;
        left: 1rem;
        z-index: 99999999999;
        background-color: transparent;
        border-radius: .25em;
        color: rgba(255,255,255,.2);
        font-size: 4em;
        transition: all 250ms ease-in-out;
        line-height: 1;

        &:hover {
            transform: scale(1.1);
            color: #fff;
        }

        &:after {
            font-family: 'Material Symbols Outlined';
            font-variation-settings: 'FILL' 1, 'wght' 700, 'GRAD' 200, 'opsz' 48;
            content: '\e1a2';
        }

    }


    #progress {
        position: absolute;
        top: 0;
        left: 0;
        z-index: 999;
        width: 100vw;
        display: flex;
        align-items: flex-end;
        flex-basis: 100%;
        cursor: pointer;

        .progress {
            width: 50%;
            background: var(--color-kmm-light-blue);
            height: 4px;
            flex: 0;
        }

    }

    #titles {

        position: absolute;
        inset: 0;
        padding: 0;
        margin: 0;
        list-style: none;

        &:before {
            position: absolute;
            inset: 0;
            display: flex;
            justify-content: center;
            align-items: center;
            pointer-events: none;
            user-select: none;
            content: 'ROUGH CUT';
            font-size: 13vw;
            font-weight: 900;
            color: rgba(255,255,255,.1);
            transform: rotate(-15deg);
        }

        li {
            display: flex;
            justify-content: center;
            align-items: center;
            position: absolute;
            inset: 0;
            transition: all 250ms ease-in-out;
            z-index: 5;
            opacity: 0;
            span {
                display: flex;
                flex-direction: column;
                justify-content: center;
                font-family: 'Inter', sans-serif;
                color: #fff;
                font-weight: 900;
                font-size: 6vw;
                line-height: .8;
                text-transform: uppercase;
                text-align: center;

                strong {
                    display: flex;
                    flex-direction: column;
                    justify-content: center;
                    text-align: center;
                }

                i {
                    display: flex;
                    flex-direction: column;
                    justify-content: center;
                    margin-top: 1rem;
                    font-style: normal;
                    font-weight: 700;
                    font-size: .25em;
                    text-align: center;
                }
            }
        }
    }


    #customSubtitles {

        display: flex;
        justify-content: center;
        align-items: center;
        position: absolute;
        inset: 0;
        transition: all 250ms ease-in-out;
        z-index: 5;

        font-family: 'Inter', sans-serif;
        color: #fff;
        font-weight: 900;
        font-size: 6vw;
        line-height: .8;
        text-transform: uppercase;
        text-align: center;


    }

}




