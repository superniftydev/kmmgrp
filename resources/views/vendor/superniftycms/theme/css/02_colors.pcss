    --color-indigo-200: hsl(228, 96%, 89%);
    --color-indigo-300: hsl(230, 94%, 82%);
    --color-indigo-400: hsl(234, 89%, 74%);
    --color-indigo-500: hsl(239, 84%, 67%);
    --color-indigo-600: hsl(243, 75%, 59%);
    --color-indigo-700: hsl(245, 58%, 51%);
    --color-indigo-800: hsl(244, 55%, 41%);
    --color-indigo-900: hsl(242, 47%, 34%);
    --color-indigo-950: hsl(244, 47%, 20%);

    --color-violet-50: hsl(250, 100%, 98%);
    --color-violet-100: hsl(251, 91%, 95%);
    --color-violet-200: hsl(251, 95%, 92%);
    --color-violet-300: hsl(252, 95%, 85%);
    --color-violet-400: hsl(255, 92%, 76%);
    --color-violet-500: hsl(258, 90%, 66%);
    --color-violet-600: hsl(262, 83%, 58%);
    --color-violet-700: hsl(263, 70%, 50%);
