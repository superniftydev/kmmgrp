    /* theme-specific */
    --color-kmm-dark-blue: hsl(219, 91%, 26%);   /* #06307D */
    --color-kmm-medium-blue: hsl(215, 91%, 29%); /* #07408E */
    --color-kmm-light-blue: hsl(211, 100%, 53%); /* #1083FF */
    --color-kmm-yellow: hsl(48, 100%, 50%);      /* #FC0    */
    --color-kmm-green: hsl(142, 71%, 45%);

    /* theme-specific hue & saturation for hsl for tints */
    --color-kmm-dark-blue-10: hsl(219, 91%, 10%);
    --color-kmm-dark-blue-20: hsl(219, 91%, 20%);
    --color-kmm-dark-blue-30: hsl(219, 91%, 30%);
    --color-kmm-dark-blue-40: hsl(219, 91%, 40%);
    --color-kmm-dark-blue-50: hsl(219, 91%, 50%);
    --color-kmm-dark-blue-60: hsl(219, 91%, 60%);
    --color-kmm-dark-blue-70: hsl(219, 91%, 70%);
    --color-kmm-dark-blue-80: hsl(219, 91%, 80%);
    --color-kmm-dark-blue-90: hsl(219, 91%, 90%);

    --color-kmm-medium-blue-10: hsl(215, 91%, 10%);
    --color-kmm-medium-blue-20: hsl(215, 91%, 20%);
    --color-kmm-medium-blue-30: hsl(215, 91%, 30%);
    --color-kmm-medium-blue-40: hsl(215, 91%, 40%);
    --color-kmm-medium-blue-50: hsl(215, 91%, 50%);
    --color-kmm-medium-blue-60: hsl(215, 91%, 60%);
    --color-kmm-medium-blue-70: hsl(215, 91%, 70%);
    --color-kmm-medium-blue-80: hsl(215, 91%, 80%);
    --color-kmm-medium-blue-90: hsl(215, 91%, 90%);

    --color-kmm-light-blue-10: hsl(211, 100%, 10%);
    --color-kmm-light-blue-20: hsl(211, 100%, 20%);
    --color-kmm-light-blue-30: hsl(211, 100%, 30%);
    --color-kmm-light-blue-40: hsl(211, 100%, 40%);
    --color-kmm-light-blue-50: hsl(211, 100%, 50%);
    --color-kmm-light-blue-60: hsl(211, 100%, 60%);
    --color-kmm-light-blue-70: hsl(211, 100%, 70%);
    --color-kmm-light-blue-80: hsl(211, 100%, 80%);
    --color-kmm-light-blue-90: hsl(211, 100%, 90%);

    --color-kmm-yellow-10: hsl(48, 100%, 10%);
    --color-kmm-yellow-20: hsl(48, 100%, 20%);
    --color-kmm-yellow-30: hsl(48, 100%, 30%);
    --color-kmm-yellow-40: hsl(48, 100%, 40%);
    --color-kmm-yellow-50: hsl(48, 100%, 50%);
    --color-kmm-yellow-60: hsl(48, 100%, 60%);
    --color-kmm-yellow-70: hsl(48, 100%, 70%);
    --color-kmm-yellow-80: hsl(48, 100%, 80%);
    --color-kmm-yellow-90: hsl(48, 100%, 90%);

    --color-kmm-green-10: hsl(142, 71%, 10%);
    --color-kmm-green-20: hsl(142, 71%, 20%);
    --color-kmm-green-30: hsl(142, 71%, 30%);
    --color-kmm-green-40: hsl(142, 71%, 40%);
    --color-kmm-green-50: hsl(142, 71%, 50%);
    --color-kmm-green-60: hsl(142, 71%, 60%);
    --color-kmm-green-70: hsl(142, 71%, 70%);
    --color-kmm-green-80: hsl(142, 71%, 80%);
    --color-kmm-green-90: hsl(142, 71%, 90%);

    /* palette */
    --color-body: var(--color-gray-700);
    --color-body-bg: var(--color-white);
    --color-link: #07408E;
    --color-link-bg: transparent;
    --color-link-active: #1083FF;
    --color-link-active-bg: transparent;

    /* navigation adjustments */
    --color-nav-background: hsl(219, 91%, 20%);
    --color-nav-background-active: hsl(219, 91%, 50%);

}


/* add classes for custom colors */
.text-kmm-dark-blue { color: var(--color-kmm-dark-blue); }
.bg-kmm-dark-blue { background-color: var(--color-kmm-dark-blue); }

.text-kmm-medium-blue { color: var(--color-kmm-medium-blue); }
.bg-kmm-medium-blue { background-color: var(--color-kmm-medium-blue); }

.text-kmm-light-blue { color: var(--color-kmm-light-blue); }
.bg-kmm-light-blue { background-color: var(--color-kmm-light-blue); }

.text-kmm-yellow { color: var(--color-kmm-yellow); }
.bg-kmm-yellow { background-color: var(--color-kmm-yellow); }

.text-kmm-green { color: var(--color-kmm-green); }
.bg-kmm-green { background-color: var(--color-kmm-green); }
