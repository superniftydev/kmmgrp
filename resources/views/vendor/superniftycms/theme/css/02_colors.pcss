    --color-fuchsia-50: hsl(289, 100%, 98%);
    --color-fuchsia-100: hsl(287, 100%, 95%);
    --color-fuchsia-200: hsl(288, 96%, 91%);
    --color-fuchsia-300: hsl(291, 93%, 83%);
    --color-fuchsia-400: hsl(292, 91%, 73%);
