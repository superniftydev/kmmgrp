    --color-red-300: hsl(0, 94%, 82%);
    --color-red-400: hsl(0, 91%, 71%);
    --color-red-500: hsl(0, 84%, 60%);
    --color-red-600: hsl(0, 72%, 51%);
    --color-red-700: hsl(0, 74%, 42%);
